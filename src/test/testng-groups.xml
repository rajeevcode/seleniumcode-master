<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name ="Regression TestSuite">
  <test name="Group Demo">
    <groups>
      <define name="all">
        <include name="bikes"></include>
        <include name="suv"></include>
        <include name="sedan"></include>
      </define>
      <define name="nobikes">
        <include name="suv"></include>
        <include name="sedan"></include>
      </define>

      <run>
        <!--<include name="nobikes"></include>-->
        <include name="all"></include>
      </run>
    </groups>
    <classes>
      <class name="testNgTutorial.TestNG_Grouping"></class>
    </classes>
  </test>
</suite>